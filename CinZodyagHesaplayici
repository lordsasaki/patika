package projeler;

import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		
		System.out.print("Dogum Tarihi: ");
		int dogumTarihi = scan.nextInt();
		
		int cinZodyagi = dogumTarihi%12;
		switch (cinZodyagi){
		case 0: System.out.print("Cin Zodyaginiz : Maymun"); break;
		case 1: System.out.print("Cin Zodyaginiz : Horoz"); break;
		case 2: System.out.print("Cin Zodyaginiz : Kopek"); break;
		case 3: System.out.print("Cin Zodyaginiz : Domuz"); break;
		case 4: System.out.print("Cin Zodyaginiz : Fare"); break;
		case 5: System.out.print("Cin Zodyaginiz : Öküz"); break;
		case 6: System.out.print("Cin Zodyaginiz : Kaplan"); break;
		case 7: System.out.print("Cin Zodyaginiz : Tavşan"); break;
		case 8: System.out.print("Cin Zodyaginiz : Ejderha"); break;
		case 9: System.out.print("Cin Zodyaginiz : Yılan"); break;
		case 10: System.out.print("Cin Zodyaginiz : At"); break;
		case 11: System.out.print("Cin Zodyaginiz : Koyun"); break;
		
		}
	}
}
